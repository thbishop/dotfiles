set mainfont {Monaco 12}
set textfont {Monaco 12}
set uifont {"Monaco Bold" 12}
set tabstop 8
set findmergefiles 0
set maxgraphpct 50
set maxwidth 16
set cmitmode patch
set wrapcomment none
set autoselect 1
set showneartags 1
set hideremotes 0
set showlocalchanges 1
set datetimeformat {%Y-%m-%d %H:%M:%S}
set limitdiffs 1
set uicolor grey85
set want_ttk 1
set bgcolor white
set fgcolor black
set colors {green red blue magenta darkgrey brown orange}
set diffcolors {red "#00a000" blue}
set markbgcolor #e0e0ff
set diffcontext 3
set selectbgcolor gray85
set extdifftool opendiff
set perfile_attrs 0
set geometry(main) 1440x852+0+22
set geometry(state) zoomed
set geometry(topwidth) 1440
set geometry(topheight) 165
set geometry(pwsash0) "557 1"
set geometry(pwsash1) "629 1"
set geometry(botwidth) 514
set geometry(botheight) 682
set permviews {}
